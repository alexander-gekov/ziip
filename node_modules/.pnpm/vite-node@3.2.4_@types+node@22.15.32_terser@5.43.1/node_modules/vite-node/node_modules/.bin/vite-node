#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/vite-node@3.2.4_@types+node@22.15.32_terser@5.43.1/node_modules/vite-node/node_modules:/workspace/node_modules/.pnpm/vite-node@3.2.4_@types+node@22.15.32_terser@5.43.1/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/vite-node@3.2.4_@types+node@22.15.32_terser@5.43.1/node_modules/vite-node/node_modules:/workspace/node_modules/.pnpm/vite-node@3.2.4_@types+node@22.15.32_terser@5.43.1/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vite-node.mjs" "$@"
else
  exec node  "$basedir/../../vite-node.mjs" "$@"
fi
