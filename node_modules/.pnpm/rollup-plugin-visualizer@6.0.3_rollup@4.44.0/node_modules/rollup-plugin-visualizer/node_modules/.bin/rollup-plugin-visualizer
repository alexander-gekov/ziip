#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3_rollup@4.44.0/node_modules/rollup-plugin-visualizer/dist/bin/node_modules:/workspace/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3_rollup@4.44.0/node_modules/rollup-plugin-visualizer/dist/node_modules:/workspace/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3_rollup@4.44.0/node_modules/rollup-plugin-visualizer/node_modules:/workspace/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3_rollup@4.44.0/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3_rollup@4.44.0/node_modules/rollup-plugin-visualizer/dist/bin/node_modules:/workspace/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3_rollup@4.44.0/node_modules/rollup-plugin-visualizer/dist/node_modules:/workspace/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3_rollup@4.44.0/node_modules/rollup-plugin-visualizer/node_modules:/workspace/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3_rollup@4.44.0/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/cli.js" "$@"
else
  exec node  "$basedir/../../dist/bin/cli.js" "$@"
fi
